package com.integracion.crud.dto;

import java.io.Serializable;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.context.annotation.PropertySource;

@PropertySource("classpath:mensajes.properties")
public class UsuarioValRequest implements Serializable {

	protected static final long serialVersionUID = 1L;
	@Value("${regexp.email}")
	protected static final String regexpEmail = "";

	@Value("${regexp.email}")
	protected static final String regexpPassword = "";
	@Value("${mensaje.error.not.blank.name}")
	protected static final String mensajeErrorNotBlankName = "";
	@Value("${mensaje.error.not.null.name}")
	protected static final String mensajeErrorNotNullName = "";
	@Value("${mensaje.error.size.name}")
	protected static final String mensajeErrorSizeName = "";
	@Value("${valor.size.min.name}")
	protected static final int valorSizeMinName =0;
	@Value("${regexp.email}")
	protected static final int valorSizeMaxName = 0;
	@Value("${mensaje.error.not.blank.email}")
	protected static final String mensajeErrorNotBlankEmail = "";
	@Value("${mensaje.error.not.null.email}")
	protected static final String mensajeErrorNotNullEmail = "";
	@Value("${mensaje.error.size.emai}")
	protected static final String mensajeErrorSizeEmail = "";
	@Value("${mensaje.error.regexp.email}")
	protected static final String mensajeErrorRegexpEmail = "";
	@Value("${valor.size.min.email}")
	protected static final int valorSizeMinEmail = 0;
	@Value("${valor.size.max.email}")
	protected static final int valorSizeMaxEmail = 0;
	@Value("${mensaje.error.not.blank.password}")
	protected static final String mensajeErrorNotBlankPassword = "";
	@Value("${mensaje.error.not.null.password}")
	protected static final String mensajeErrorNotNullPassword = "";
	@Value("${mensaje.error.size.password}")
	protected static final String mensajeErrorSizePassword = "";
	@Value("${mensaje.error.regexp}")
	protected static final String mensajeErrorRegexpPassword = "";
	@Value("${valor.size.min.password}")
	protected static final int valorSizeMinPassword = 0;
	@Value("${valor.size.max.password}")
	protected static final int valorSizeMaxPassword = 0;

}
